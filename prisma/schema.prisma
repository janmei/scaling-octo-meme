// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Order {
  id       String @id
  customer String
  location String
  product  String
  quantity Int
  total    Float
  status   String
  date     String
}

model Shipment {
  id       String  @id
  status   String
  eta      String?
  courier  String?
  progress Int
  color    String
}
